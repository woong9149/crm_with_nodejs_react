{
    "name": "management",
    "version": "1.0.0",
    "script": {
        "client": "cd client && yarn start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "nodemon": "^2.0.4",
        "pm2": "^4.4.0",
        "yarn": "^1.22.4"
    },
    "devDependencies": {
        "concurrently": "^4.0.1"
    }
}
